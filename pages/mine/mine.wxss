    margin-top: 16rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #8392A0;
    line-height: 27px;
    height: 54rpx;
}
.my-demand-list .item{
    margin-left: 32rpx;
    padding-top:16rpx;
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #8392A0;
}
.my-demand-list .item.remuneration{
    padding-top:8rpx;
}
.my-demand-list .see-btn{
    position: absolute;
    right: 32rpx;
    bottom: 32rpx;
    width: 168rpx;
    height: 56rpx;
    line-height:56rpx;
    background-color: #FFBA20;
    font-size:24rpx;
    color: #ffffff;
    text-align: center;
    border-radius: 28rpx;

}


