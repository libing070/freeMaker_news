/* pages/mine/mine.wxss */
page {
    background-color: #F7F8FC;

}

.mine{
    position: relative;
    height: 100%;
}
.head-img{
    width: 100%;
    top: 0;
    left: 0;
    position: absolute;
    z-index: -1;
}
.user {
    position: relative;
    display: flex;
    align-items: center;
    padding-bottom: 64rpx;
}

.avatarBox {
    width: 96rpx;
    height: 96rpx;
    position: relative;
    margin-left: 44rpx;
    flex-shrink: 0;
    margin-right: 20rpx;
}

.avatarBox .avatar {
    border-radius: 50%;
    width: 100%;
    height: 100%;
    position: absolute;
    border: 4rpx solid #FFBA20;
}
.name-box{
    position: relative;
    display: flex;
    flex-direction: column;

}
.name-user {
    font-family: PingFangSC-Medium;
    font-size: 36rpx;
    color: #262626;
    font-weight: bold;
}

.name-tag {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #8392A0;
    margin-top: 8rpx;
}
.price{
    position: absolute;
    right: 0;
    top: 0;
    margin:auto;
    height: 96rpx;
    padding-left: 18rpx;
    padding-right: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    background-color: #FFBA20;
    border-radius: 50rpx 0 0 50rpx;
}
.price .wallet{
    width: 56rpx;
    height: 56rpx;
    position: relative;
    margin-right: 10rpx;
    /* background-color: rgba(255,0,0,0.3) */
}
.price .wallet .icon{
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin:auto;
    width: 60rpx;
    height: 60rpx;
}
.price .mm{
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: center;

}
.price .mm .top{
    margin-top: 6rpx;
    font-size: 20rpx;
    font-family: PingFangSC-Regular;
    color: #FFFFFF;
}
.price .mm .bottom{
    margin-bottom: 4rpx;
}
.price .mm .bottom .value{
    font-family: DINAlternate-Bold;
    font-weight: bold;
    font-size: 50rpx;
    color: #FFFFFF;
}
.price .mm .bottom .unit{
    font-size: 24rpx;
    font-family: PingFangSC-Regular;
    color: #FFFFFF;
}

.tabs {
    position: relative;
    border-top: 1px solid #e6e6e6;
    background: #FFFFFF;
    box-shadow: 0 1px 0 0 rgba(0,0,0,0.03);
    border-radius: 8px 8px 0 0;
    width: 100vw;
    height: 44px;
    display: flex;
    justify-content: space-around;
    z-index: 1;
    border-bottom: 1px solid #e6e6e6;
}
.tabs.tabs-sticky{
    position: sticky;
}
.tabs .item {
    font-family: PingFangSC-Regular;
    font-size: 28rpx;
    color: #8392A0;
    text-align: center;
    box-sizing: border-box;
    height: 44px;
    line-height: 44px;
 
}

.tabs .item.sel {
    font-family: PingFangSC-Medium;
    font-size: 28rpx;
    color: #4F4F4F;
    font-weight: 500;
    position: relative;
}

.tabs .item.sel::after {
    content: "";
    background: linear-gradient(90deg, #4F4F4F, #4F4F4F);
    border-radius: 4rpx;
    width: 40rpx;
    height: 4rpx;
    left: 0;
    right: 0;
    bottom: 0px;
    position: absolute;
    margin: auto;
}
.demand-second-level{
    height: 44px;
    position: relative;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    z-index: 1;
    background-color: #ffffff;
    padding-left: 40rpx;
    box-shadow: 0 1px 0 0 rgba(0,0,0,0.03);
}

.demand-second-level.sticky{
    position: sticky;
}
.demand-second-level .level{
    padding: 0 34rpx;
    margin-right: 20rpx;
    height: 56rpx;
    line-height: 56rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #8392A0;
}
.demand-second-level .level.sel{
    background: #EEF5FD;
    border-radius: 34rpx;
    font-family: PingFangSC-Medium;
    font-size: 24rpx;
    color: #6F96BA;

}

.my-demand-list{
    position: relative;
    background-color: white;
    box-shadow: 0 2px 8px 0 rgba(36, 19, 72, 0.04);
    margin:20rpx;
    border-radius: 8rpx;
}

.my-demand-list .title{
    position: relative;
    padding-top:44rpx;
    margin-left: 32rpx;
}
.my-demand-list .title .tag{
    position: absolute;
    color: #8392A0;
    top: 44rpx;
    line-height: 27px;
    bottom: 0;
    margin: auto;
