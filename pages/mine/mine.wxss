.order-container{
    position: relative;
}
.order-container .no-data{
    margin-top:200rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.order-container .no-data .icon{
    width: 150rpx;
    height: 150rpx;
}
.order-container .no-data .desc{
    width: 420rpx;
    margin-top:30rpx ;
    font-size: 30rpx;
    color: #7D7D7D;
}
.order-list .dropdown-menu{
    position: relative;
}
.dropdown-menu{
    position: relative;
    border-bottom: 1px solid #E6E6E6;
}
.dropdown-menu.sticky{
    position: sticky;
    z-index: 1;
}
.dropdown-menu .menu{
    position: relative;
    z-index: 999;
    background-color: white;
    position: relative;
    height: 44px;
    line-height: 44px;
    padding-left: 40rpx;
    display: flex;
    flex-direction: row;
    justify-content:flex-start;
}
.dropdown-menu .menu .menu-item{
    width: 160rpx;
    height: 44rpx;
    line-height: 44px;
    margin-right: 20rpx;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;

}
.dropdown-menu .menu .menu-item .name{
    height: 44rpx;
    line-height: 44rpx;
}
.dropdown-menu .menu .menu-item .icon{
    vertical-align: middle;
    margin-top: -4rpx;
    width: 20rpx;
    height: 20rpx;
}

.dropdown-menu .shade-cell {
    background-color: rgba(0, 0, 0, 0.5);
    top: 44px;
    left: 0;
    right: 0;
    height: 1000px;
    bottom: 0;
    position: absolute;
    z-index: 2;
}
 .shade-cell .shade-cell-main {
    background-color: white;
    border-radius: 0 0 10px 10px;
    width: 100%;
    top: 0;
    position: absolute;
    animation: main-shows 0.25s forwards;
    display: flex;
    flex-direction: column;
    /* flex-wrap: wrap; */
    padding-bottom: 40rpx;
}

@keyframes main-shows {
    0% {
        transform: translateY(-20%);
        opacity: 0;
    }
    100% {
        transform: translateY(0);
        opacity: 1;
    }
}
.shade-cell-main .shade-item-option{
    display: inline-block;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
    padding-left: 40rpx;
    padding-right: 40rpx;
    height: 96rpx;
    line-height: 96rpx;
    box-shadow: 0 1px 0 0 #E6E6E6;
}
.shade-cell-main .shade-item-option .sel{
    color: #FFBA20;
}
.shade-cell-main .shade-item-option .checked{
    width: 30rpx;
    height: 30rpx;
}

