<!--pages/pushUserList/pushUserList.wxml-->

<view style="padding-bottom: {{safeBottom}}px;">
    <view class="notice">
        <image class="icon" src="/images/common/icon-warning.png"></image>
        <text class="text">选择一个Freelancer,系统会自动取消其他人的订单。</text>
    </view>

    <view class="box-cell" wx:for="{{10}}" wx:key="index" catchtap="tapToProductDetails">
        <image class='photo' src="/images/common/715_2.jpg" mode="aspectFill"></image>
        <view class="user-box">
            <view class="user">
                <view class="avatarBox">
                    <image class="avatar" src="/images/common/default-head.png"></image>
                </view>
                <view>
                    <view class="name-user">波英冰</view>
                    <view  class="name-tag>">xxxx | 4.9分</view>
                </view>
            </view>
            <view class="mark-type">
                <scroll-view class="menu" scroll-x="true">
                    <view class="item" wx:for="{{10}}" wx:key="index" data-id="{{index}}" catchtap="tapTohomeList">
                        <text class="name">更多类型{{index+1}}</text>
                    </view>
                </scroll-view>  
            </view>
        </view>
        <view class="price">
            <text class="unit">¥</text>
            <text class="value">400</text>
            <text class="unit">/时</text>
        </view>
    </view>
</view>