/* pages/orderDetails/orderDetails.wxss */
page {
    background-color: #F7F8FC;
}
.order-status{
    position: relative;
    padding: 40rpx 20rpx;
    background-color: #e6e6e6;
}
.order-status .status{
    color: #000000;
    font-weight: bold;
    font-size: 34rpx;
}
.order-status .desc{
    font-size: 28rpx;
    margin-top:10rpx;
}

.order-status .uu{
    position: absolute;
    top: 0;
    bottom: 0;
    right: 40rpx;
    margin:auto;
    width: 250rpx;
    height: 200rpx;
}
.order-status .uu .earth{
    position: absolute;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    margin:auto;
    width: 150rpx;
    height: 150rpx;
}
.order-steps{
    position: relative;
    height: 182rpx;
    margin-left: 40rpx;
    margin-right: 40rpx;
    margin-top: 24rpx;
    background-color: white;
    border-radius: 8rpx;
    box-shadow: 0 2px 18px 0 rgba(227,229,231,0.71);
    display: flex;
}
.steps{
    position: relative;
    display: flex;
    margin:auto;
    flex-direction: row;
    justify-content: space-between;
}
.steps .step-default-line{
    position: absolute;
    top: 28rpx;
    left: 0;
    right: 0;
    margin:auto;
    height: 6rpx;
    width: 85%;
    background-color: #E6E6E6;
    z-index: 0;
}
.steps .step-finish-line{
    position: absolute;
    top: 28rpx;
    left: 60rpx;
    margin:auto;
    height: 6rpx;
    /* width: 70%; */
    background-color: #FFBA20;
    z-index: 2;
}
.steps .step{
    position: relative;
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    z-index: 3;
}
.steps .step .step-item{
    position: relative;
    width: 90rpx;
    height: 60rpx;
    border-radius: 100%;

}
.steps .step .step-item .status-icon{
    width: 32rpx;
    height: 32rpx;
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    margin:auto;
}
.steps .step .step-title{
    color: #8392A0;
    font-size: 24rpx;
    text-align: center;
    margin-top: 8rpx;
    height: 60rpx;
    width: 85rpx;
}
.steps .step .step-title.curr{
    color: #0C2032;
}

