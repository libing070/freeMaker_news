/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/* mark: 自定义导航栏 */
.navigationBar {
  background-color: white;
  font-size: 36rpx;
  color: #0C2032;
  font-weight: 500;
  text-align: center;
  line-height: 44px;
  white-space: nowrap;
  text-overflow: ellipsis;
  height: 44px;
  top: 0;
  left: 0;
  right: 0;
  position: fixed;
  padding: 0 105px;
  overflow: hidden;
  z-index: 1;
}

.navigationBar.clear {
  background-color: transparent;
  color: #0C2032;
}

.navigationBar navigator {
  background-repeat: no-repeat;
  background-size: 20px;
  background-position: center;
  width: 40px;
  height: 40px;
  left: 2px;
  bottom: 2px;
  position: absolute;
}

.navigationBar navigator.white {
  background-image: url(data:image/png;base64,);
}

.navigationBar navigator.black {
  background-image: url(data:image/png;base64,);
}

